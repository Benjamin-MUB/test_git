pipeline {
  agent any
  stages {
    stage('begin') {
      steps {
        echo 'hello world'
      }
    }
    stage('Test') {
      steps {
        sh 'lllllllllll'
      }
    }
    stage('Last') {
      steps {
        sh 'echo \'B2OBA\''
      }
    }
  }
  post {
    always {
      echo 'finish'
      
    }
    
  }
}